{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "*"
      ],
      "enabled": false
    },
    {
      "description": "Python replacement",
      "matchDatasources": ["docker"],
      "matchPackageNames": [
        "bitnami/grafana"
      ],
      "replacementName": "grafana/grafana",
      "enabled": true
    }
  ]
}
