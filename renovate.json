{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "*"
      ],
      "enabled": false
    },
    {
      "description": "Python replacement",
      "matchDatasources": ["docker"],
      "matchPackageNames": [
        "amd64/python",
        "arm64v8/python",
      ],
      "replacementName": "amd64/python",
      "overridePackageName": "amd64/python",
      "overrideDepName": "amd64/python",
      "enabled": true
    },
  ],
}
