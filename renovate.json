{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Python replacement",
      "matchDatasources": ["docker"],
      "matchPackageNames": [
        "amd64/python",
        "arm64v8/python"
      ],
      "replacementName": "arm64v8/python",
      "overridePackageName": "arm64v8/python",
      "overrideDepName": "arm64v8/python"
    }
  ]
}
