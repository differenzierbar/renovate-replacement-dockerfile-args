{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "*"
      ],
      "enabled": false
    },
    {
      "description": "replace ubuntu python with python",
      "matchDatasources": ["docker"],
      "matchPackageNames": [
        "docker.io/ubuntu/python"
      ],
      "replacementName": "docker.io/python",
      "enabled": true
    }
  ]
}
